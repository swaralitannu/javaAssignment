import java.util.Arrays;
class Main{
    public static void main(String args[]){
    int arr[]={20,7,49,79,1,53};
	
	      Arrays.sort(arr);
			      System.out.println();
			      System.out.println("Array After Sorting");
			      for(int i=0;i<arr.length;i++)
			      {
				        System.out.print(" "+arr[i]);
			       }
    }
}

